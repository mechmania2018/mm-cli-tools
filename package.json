{
  "name": "mechmania",
  "version": "0.0.4",
  "description": "CLI tools to do everything mechmania (test your bot, upload them to mm servers, queue games, etc.)",
  "main": "index.js",
  "repository": "https://github.com/pranaygp/mm-cli-tools",
  "author": "Pranay Prakash <pranay.gp@gmail.com>",
  "license": "MIT",
  "scripts": {
    "flow": "flow src/",
    "build": "flow-remove-types src/ --out-dir lib"
  },
  "bin": {
    "mm": "lib/index.js"
  },
  "pkg": {
    "targets": [
      "node8-macos",
      "node8-linux",
      "node8-win"
    ],
    "scripts": [
      "lib/commands"
    ]
  },
  "dependencies": {
    "app-root-path": "2.1.0",
    "chalk": "2.4.0",
    "mkdirp": "0.5.1",
    "node-fetch": "2.0.0",
    "tar": "4.4.6",
    "through2": "2.0.3",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "eslint": "4.18.0",
    "flow-bin": "0.66.0",
    "flow-remove-types": "1.2.3",
    "pkg": "4.3.0"
  }
}
